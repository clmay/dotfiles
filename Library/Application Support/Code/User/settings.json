{
  "breadcrumbs.enabled": true,
  // "C_Cpp.clang_format_style": "{ BasedOnStyle: WebKit, UseTab: Never, IndentWidth: 2, TabWidth: 2, BreakBeforeBraces: Attach, AllowShortIfStatementsOnASingleLine: false, IndentCaseLabels: false, AccessModifierOffset: -2 }",
  // "C_Cpp.default.cppStandard": "c++11",
  "editor.fontFamily": "Monaco, monospace",
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.minimap.enabled": true,
  "editor.renderWhitespace": "boundary",
  "editor.rulers": [80, 100, 120],
  "editor.suggestSelection": "first",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "files.associations": {
    "Brewfile": "shellscript",
    "zshrc": "shellscript",
    "*.leex": "HTML (Eex)"
  },
  "files.autoSave": "onFocusChange",
  "files.exclude": {
    "**/.git": false,
    "**/.hg": false,
    "**/.svn": false,
    "**/CVS": false
  },
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "html.format.wrapLineLength": 80,
  "markdown.extension.italic.indicator": "_",
  "markdown.extension.tableFormatter.normalizeIndentation": true,
  "prettier.proseWrap": "always",
  "prettier.semi": false,
  "prettier.singleQuote": true,
  "solargraph.formatting": true,
  "telemetry.enableTelemetry": false,
  "telemetry.enableCrashReporter": false,
  "terminal.external.osxExec": "Alacritty.app",
  "terminal.integrated.commandsToSkipShell": [
    "workbench.action.nextEditor",
    "workbench.action.previousEditor"
  ],
  "window.zoomLevel": 0,
  "workbench.colorTheme": "Community Material Theme Darker High Contrast",
  "workbench.editor.showTabs": true,
  "workbench.iconTheme": "eq-material-theme-icons",
  "workbench.startupEditor": "none",
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.tabSize": 2
  },
  "[ruby]": {
    "editor.tabSize": 2
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
